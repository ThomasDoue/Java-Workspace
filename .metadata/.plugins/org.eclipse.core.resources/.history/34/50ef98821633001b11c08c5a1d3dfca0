
public class Fizzbuzz {

    public static void main(String args[]){
    	
    	int i;
    	int x=9;
    	
        for (i = 1; i <= 100; i++){
            if ((i % 3 == 0) && (i % 5 == 0)){
                System.out.println("FizzBuzz" + " ");
            } else if(i==x) {
                System.out.println(i % 3 == 0 ? "fizz \n": x +"\n");
            } else if(i% 5 == 0){
                System.out.println("Buzz" + " ");
            }else if ( i%3==0) {
            } else {
                System.out.println(x);
            }
        }
    }

}


