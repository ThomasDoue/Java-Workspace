
public class Fizzbuzz {

    public static void main(String args[]){
    	
    	int x;
    	int i=9;
    	
        for (int x = 1; x <= 100; x++){
            if ((x % 3 == 0) && (x % 5 == 0)){
                System.out.println("FizzBuzz" + " ");
            } else if(x==i) {
                System.out.println(x % 3 == 0 ? \"fizz \n": i +"\n");
            } else if(x % 5 == 0){
                System.out.println("Buzz" + " ");
            }else if ( x%3==0= ) {
            } else {
                System.out.println(x);
            }
        }
    }

}


