
public class Fizzbuzz {

    public static void main(String args[]){
    	
    	int i;
    	int x=9
    	
        for (int x = 1; x <= 100; x++){
            if ((x % 3 == 0) && (x % 5 == 0)){
                System.out.println("FizzBuzz");
            } else if(x % 3 == 0 ? "fizz \n" : x +"\n"){
                System.out.println("Fizz");
            } else if(x % 5 == 0){
                System.out.println("Buzz");
            } else {
                System.out.println(x);
            }
        }
    }

}


